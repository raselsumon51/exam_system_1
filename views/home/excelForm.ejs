<!-- make form to input excel file and hidden exam id  -->
<form action="/excel/<%= examId %>/" method="POST" enctype="multipart/form-data" class="w-screen px-4 mt-10">
    <h3 class="text-3xl font-semibold text-center mb-6 text-gray-800">Add Questions by Excel</h3>
    <input type="file" name="excel" class="w-full py-2 px-4 border border-gray-300 rounded-md focus:outline-none focus:ring-2 focus:ring-blue-400">
    <input type="hidden" name="examId" value="<%= examId %>">
    <button type="submit" class="block w-full py-2 px-4 bg-blue-600 text-white font-medium rounded-md shadow-md hover:bg-blue-500 focus:outline-none focus:ring-2 focus:ring-blue-400">Upload Excel</button>
</form>


